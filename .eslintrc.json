{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-single"],
		"no-console": "warn",
		"no-duplicate-imports":"error"
		
	 }
}
